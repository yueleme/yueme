	        super.onCreate(savedInstanceState);
	        setContentView(R.layout.activity_my_request);
	    }
	 
	
	 private void initView(){
		 tab_put = (TextView) findViewById(R.id.tab_put);
		 tab_put = (TextView) findViewById(R.id.tab_get);
	 }

}
